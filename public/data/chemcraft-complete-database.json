        }
      },
      {
        "rule_id": "R004",
        "name": "Hydrogen + Nonmetal → Binary Compound",
        "pattern": {
          "element1": ["H"],
          "element2_type": ["nonmetal"],
          "elements2": ["O", "S", "N", "F", "Cl", "Br", "I"]
        },
        "prediction": {
          "will_react": true,
          "probability": 0.9,
          "product_type": "binary_molecular",
          "state": "varies",
          "reaction_type": "synthesis",
          "energy": "exothermic"
        }
      },
      {
        "rule_id": "R005",
        "name": "Noble Gas + Any → No Reaction",
        "pattern": {
          "element1": ["He", "Ne", "Ar", "Kr"],
          "element2_type": ["any"]
        },
        "prediction": {
          "will_react": false,
          "probability": 0.0,
          "reason": "Noble gases are chemically inert"
        }
      },
      {
        "rule_id": "R006",
        "name": "Xenon + Fluorine → Xenon Fluoride",
        "pattern": {
          "element1": ["Xe"],
          "element2": ["F"]
        },
        "prediction": {
          "will_react": true,
          "probability": 0.7,
          "product_type": "noble_gas_compound",
          "possible_products": ["XeF2", "XeF4", "XeF6"],
          "state": "solid",
          "reaction_type": "synthesis",
          "conditions": "high temperature/pressure"
        }
      },
      {
        "rule_id": "R007",
        "name": "Transition Metal + Transition Metal → Alloy",
        "pattern": {
          "element1_type": ["transition_metal"],
          "element2_type": ["transition_metal"]
        },
        "prediction": {
          "will_react": false,
          "probability": 0.15,
          "note": "Forms metallic mixture (alloy), not chemical compound"
        }
      },
      {
        "rule_id": "R008",
        "name": "Active Metal + Water → Hydroxide + H2",
        "pattern": {
          "element1_type": ["alkali_metal", "alkaline_earth"],
          "element2": ["O"],
          "element3": ["H"],
          "conditions": "water_present"
        },
        "prediction": {
          "will_react": true,
          "probability": 1.0,
          "product_type": "hydroxide",
          "formula_pattern": "{metal}OH",
          "byproduct": "H2",
          "violence": "very_high"
        }
      }
    ],

    "formula_generation_rules": {
      "oxidation_states": {
        "H": [1, -1],
        "Li": [1], "Na": [1], "K": [1], "Rb": [1], "Cs": [1],
