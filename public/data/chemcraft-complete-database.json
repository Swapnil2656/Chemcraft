        "rule_id": "R003",
        "name": "Metal + Oxygen → Metal Oxide",
        "pattern": {
          "element1_type": ["metal"],
          "element2": ["O"],
          "exclude": ["Au", "Pt", "Ag"]
        },
        "prediction": {
