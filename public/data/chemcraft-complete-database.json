        "enabled": true,
        "priority": 0,
        "api_endpoint": "https://pubchem.ncbi.nlm.nih.gov/rest/pug",
        "rate_limit_ms": 200,
        "cache_enabled": true,
        "fallback_on_error": true
      }
    },

    "prediction_workflow": [
      {
        "step": 1,
        "action": "check_pubchem_api",
        "on_success": "return_verified_data",
        "on_failure": "proceed_to_step_2"
      },
      {
        "step": 2,
        "action": "apply_chemistry_rules",
        "confidence_required": 0.8,
        "on_success": "return_predicted_data",
        "on_failure": "proceed_to_step_3"
      },
      {
        "step": 3,
        "action": "run_ml_model",
        "confidence_required": 0.6,
        "on_success": "return_ml_prediction",
        "on_failure": "return_no_reaction"
      }
    ]
  },

  "chemistry_rules_engine": {
    "reactivity_rules": [
      {
        "rule_id": "R001",
        "name": "Alkali Metal + Halogen → Ionic Salt",
        "pattern": {
          "element1_type": ["alkali_metal"],
          "element2_type": ["halogen"],
          "elements1": ["Li", "Na", "K", "Rb", "Cs"],
          "elements2": ["F", "Cl", "Br", "I"]
