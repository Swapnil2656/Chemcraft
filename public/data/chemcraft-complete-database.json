        "O": [-2], "S": [6, 4, 2, -2],
        "F": [-1], "Cl": [7, 5, 3, 1, -1],
        "Br": [7, 5, 3, 1, -1], "I": [7, 5, 3, 1, -1],
        "Fe": [2, 3], "Cu": [1, 2], "Zn": [2],
        "Ag": [1], "Au": [1, 3], "Pb": [2, 4]
      },
      
      "formula_balancing": {
